{
  "expo": {
    "name": "EverydayBudget",
    "slug": "EverydayBudget",
    "privacy": "public",
    "platforms": [
      "ios"
    ],
    "version": "1.5.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.everydaybudget.everydaybudget"
    }
  }
}
